<Window x:Class="VideoDBMS.formSignup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoDBMS"
        mc:Ignorable="d"
        Title="formSignup" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="111,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="111,87,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <DatePicker x:Name="datBirthdate" HorizontalAlignment="Left" Margin="111,126,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnSignup" Content="Signup" HorizontalAlignment="Left" Margin="111,181,0,0" VerticalAlignment="Top" Width="75" Click="btnSignup_Click"/>
        <Label x:Name="lblName" Content="User name" HorizontalAlignment="Left" Margin="49,48,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="55,87,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDate" Content="Birth date" HorizontalAlignment="Left" Margin="49,125,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
